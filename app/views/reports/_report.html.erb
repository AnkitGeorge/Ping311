<ul>
    <h4><li><%= link_to image_tag(report.image), report_path(report) %></li></h4>
    <h4><li>Address: <%= link_to "#{report.address}", report_path(report) %></h4></p>
    <p><li>Description: <%= report.description  %></li></p>
    <p><li>Status: <%= statusName(report.status) %></li></p>

    <%= link_to like_report_path(report), class: "like", method: :put do %>
      <button type="button">
        <span class="badge"><%= report.get_upvotes.size %></span>
      </button>
    <% end %>
    <br>
    <br>
</ul>
